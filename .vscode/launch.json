{
  // Используйте IntelliSense, чтобы узнать о возможных атрибутах отладки Node.js.
  // Наведите указатель мыши, чтобы просмотреть описания существующих атрибутов.
  // Для получения дополнительной информации посетите: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "node",
      "request": "launch",
      "name": "Launch import in debug",
      "program": "${workspaceFolder}/server/cli-dev.js",
      "args": [
        "v1migrate",
        "--from",
        "mongodb://10.170.11.232,10.170.11.87,10.170.108.85,10.170.108.86/trust?replicaSet=rep01"
      ],
      "runtimeArgs": [
        "--inspect=9229"
      ],
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch rev-sync in debug",
      "program": "${workspaceFolder}/server/cli-dev.js",
      "args": [
        "rev-sync",
        "-d",
        "1"
      ],
      "runtimeArgs": [
        "--inspect=9229"
      ],
      "port": 9229,
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "techcheck:*"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch in Debug",
      "program": "${workspaceRoot}/server/server-dev.js",
      "runtimeArgs": [
        "--inspect=9229"
      ],
      "port": 9229,
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "techcheck:*"
      },
      "restart": true,
      "sourceMaps": true
    }, {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "args": [
        "-u",
        "tdd",
        "--timeout",
        "999999",
        "--colors",
        "--require",
        "@babel/register",
        "--slow",
        "300",
        "--recursive",
        "${workspaceRoot}/tests"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "NODE_ENV": "test"
      }
    }, {
      "type": "node",
      "request": "attach",
      "name": "Attach to API",
      "port": 9229
    }, {
      "type": "node",
      "request": "attach",
      "name": "Attach to Cron",
      "port": 9230
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Queue",
      "port": 9231
    }
  ]
}
